{
  "name": "bsb-server",
  "version": "1.0.0",
  "description": "bsbAPI Server for the bsbJS library",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/easyBSB/bsbServer"
  },
  "scripts": {
    "build": "yarn tsc && babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "tsc": "tsc --project tsconfig.compile.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" --exec babel-node --extensions .ts src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "bundle": "yarn tsc && cross-env NODE_ENV=production webpack",
    "start:bundle": "cross-env NODE_ENV=production node dist/app.bundle.js",
    "test:lint": "eslint src --ext .ts",
    "test:lint:fix": "eslint src --ext .ts --fix"
  },
  "dependencies": {
    "@easybsb/bsbdef": "^0.0.1",
    "@tsed/ajv": "^6.22.2",
    "@tsed/common": "^6.22.2",
    "@tsed/core": "^6.22.2",
    "@tsed/di": "^6.22.2",
    "@tsed/exceptions": "^6.22.2",
    "@tsed/json-mapper": "^6.22.2",
    "@tsed/platform-express": "^6.22.2",
    "@tsed/schema": "^6.22.2",
    "@tsed/socketio": "^6.22.2",
    "@tsed/swagger": "^6.22.2",
    "ajv": "^7.0.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "method-override": "^3.0.0",
    "swagger-ui-dist": "4.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@tsed/cli-plugin-eslint": "2.8.1",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/method-override": "0.0.31",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.20",
    "@types/socket.io": "^2.1.12",
    "@tsed/schema": "^6.22.2",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^5.13.0",
    "webpack-cli": "^4.3.1"
